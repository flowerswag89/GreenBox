<Window x:Class="GreenBox.Views.SwagTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GreenBox.Views"
        xmlns:controls="clr-namespace:GreenBox.Controls"
        mc:Ignorable="d"
        Title="GreenBox" Icon="../Resources/DefaultImage/GreenBoxLogo1.jpg" ResizeMode="NoResize" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <MediaElement Source="{Binding MediaSource}"/>
        <controls:SwagTester Visibility="Visible"></controls:SwagTester>
    </Grid>
</Window>
